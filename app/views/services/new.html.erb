<div class="container">
  <div class="row">
     <div class="col-xs-12 col-sm-6 col-sm-offset-3">
        <h3>Create a New Service</h3>
        <%= simple_form_for(@service) do |f| %>
        <%= f.input :category %>
        <%= f.input :price %>
        <%= f.button :submit, :class => "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
